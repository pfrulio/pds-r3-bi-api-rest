<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="fr.esiag.isies.pds.model.HealthPath"
		table="tr_health_path_bi">
		<!-- medicalAct currentState startDate endDate -->
		<id name="id" type="int">
			<column name="idtr_health_path" precision="5" scale="0" />
			<generator class="increment" />
		</id>
		<property name="endDate" type="date">
			<column name="tr_health_path_end_date" length="45" not-null="false" />
		</property>

		<many-to-one name="medicalRecords"
			class="fr.esiag.isies.pds.model.MedicalRecords"
			lazy="false" cascade="all">
			<column name="IDTR_MEDICAL_RECORD" />
		</many-to-one>
	</class>
</hibernate-mapping>